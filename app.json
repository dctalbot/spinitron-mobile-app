{
  "expo": {
    "name": "WXYZ-FM",
    "description": "Discover new music from WXYZ.",
    "slug": "wxyz-app",
    "githubUrl": "https://github.com/dctalbot/spinitron-mobile-app",
    "privacy": "public",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "automatic",
    "splash": {
      "image": "./assets/splash/default.png",
      "resizeMode": "contain",
      "backgroundColor": "#212733"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": ["**/*"],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "org.wxyz",
      "infoPlist": {
        "UIBackgroundModes": ["audio"]
      }
    },
    "android": {
      "package": "org.wxyz",
      "playStoreUrl": "https://play.google.com/store/apps/details?id=org.wxyz",
      "splash": {
        "backgroundColor": "#212733",
        "resizeMode": "contain",
        "mdpi": "./assets/splash/mdpi.png",
        "hdpi": "./assets/splash/hdpi.png",
        "xhdpi": "./assets/splash/xhdpi.png",
        "xxhdpi": "./assets/splash/xxhdpi.png",
        "xxxhdpi": "./assets/splash/xxxhdpi.png"
      },
      "permissions": []
    },
    "platforms": ["android", "ios"],
    "extra": {
      "eas": {}
    },
    "runtimeVersion": {
      "policy": "appVersion"
    }
  }
}
